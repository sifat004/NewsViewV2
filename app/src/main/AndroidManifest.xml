<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sifatullahchowdhury.rokomari.newsview">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".applicaton.MyApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".splash_screen_intro_slider.PrimarySplashScreenActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".home_activity.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait"

            />
        <activity
            android:name=".splash_screen_intro_slider.SecondarySplashScreenActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"

            />
        <activity
            android:name=".show_full_article_activity.ShowArticleActivity"
            android:screenOrientation="portrait"
            android:label="@string/Read_full_article"


            />
        <activity
            android:name=".trivia_activity.TriviaActivity"
            android:screenOrientation="portrait"
            android:label="@string/trivia"


            />

    </application>

</manifest>